{
  "name": "chamber-management-system",
  "version": "1.0.0",
  "description": "Doctors chember management system",
  "main": "server/app.js",
  "engines": {
    "node": "16.13.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/app.js",
    "server": "nodemon server/app.js",
    "client": "npm start --prefix client",
    "watch": "npm run sass:watch --prefix client",
    "tailwind": "npm run tailwind --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "develop": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\" \"npm run watch\" \"npm run tailwind\" "
  },
  "author": "azizulhasan",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "custom-env": "^2.0.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.1.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.2",
    "nodemon": "^2.0.15"
  }
}
